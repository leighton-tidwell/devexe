{
  "extends": ["next/core-web-vitals", "airbnb", "airbnb-typescript"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "overrides": [
    {
      "files": "*",
      "rules": {
        "import/prefer-default-export": "off",
        "import/no-cycle": "off",
        "linebreak-style": "off",
        "react/jsx-props-no-spreading": "off",
        "react/react-in-jsx-scope": "off",
        "@next/next/no-img-element": "off"
      }
    }
  ]
}
